<query id="insertContent" action="insert">
	<tables>
		<table name="nstore_digital_contents" />
	</tables>
	<columns>
		<column name="file_srl" var="file_srl" filter="number" notnull="notnull" />
		<column name="item_srl" var="item_srl" notnull="notnull" />
		<column name="module_srl" var="module_srl" />
		<column name="extra_vars" var="extra_vars" />
		<column name="list_order" var="list_order" />
		<column name="regdate" var="regdate" default="curdate()" />
	</columns>
</query>
