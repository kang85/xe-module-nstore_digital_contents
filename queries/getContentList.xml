<query id="getContentList" action="select">
	<tables>
		<table name="nstore_digital_contents" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
	<conditions>
		<condition operation="equal" column="item_srl" var="item_srl" />
	</conditions>
	<navigation>
		<index var="sort_index" default="list_order" order="asc" />
	</navigation>
</query>
