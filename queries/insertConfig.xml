<query id="insertConfig" action="insert">
	<tables>
		<table name="nstore_digital_contents_config" />
	</tables>
	<columns>
		<column name="item_srl" var="item_srl" filter="number" notnull="notnull" />
		<column name="period_type" var="period_type" />
		<column name="period" var="period" filter="number" />
		<column name="extra_vars" var="extra_vars" />
		<column name="regdate" var="regdate" default="curdate()" />
	</columns>
</query>
